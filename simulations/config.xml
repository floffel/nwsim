<root>
    <AnalogueModels>
        <AnalogueModel type="SimplePathlossModel" thresholding="true">
            <parameter name="alpha" type="double" value="2.0"/>
        </AnalogueModel>
        <AnalogueModel type="SimpleObstacleShadowing">
			<parameter name="carrierFrequency" type="double" value="5.890e+9"/>
			<obstacles>
				<type id="building" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.apartments" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.cathedral" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.church" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.college" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.commercial" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.construction" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.container" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.garage" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.garages" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.government" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.guardhouse" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.historic" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.hospital" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.hotel" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.industrial" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.kindergarten" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.office" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.public" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.residential" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.retail" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.roof" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.school" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.service" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.shed" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.ship" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.synagogue" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.tech_cab" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.theatre" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.transportation" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.university" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.warehouse" db-per-cut="9" db-per-meter="0.2" />
				<type id="building.yes" db-per-cut="9" db-per-meter="0.2" />
			</obstacles>
		</AnalogueModel>
    </AnalogueModels>
    <Decider type="Decider80211p">
        <!-- The center frequency on which the phy listens-->
        <parameter name="centerFrequency" type="double" value="5.890e9"/>
    </Decider>
</root>
